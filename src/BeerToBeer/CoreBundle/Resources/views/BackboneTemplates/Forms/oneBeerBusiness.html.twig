<div class="row oneBeerBusiness" id="<%= id %>">
	<div class="col-xs-3 volume">
		<%= volume %> cl
	</div>
	<div class="col-xs-3 prix">
		<% var outputNormal = "-"; if(typeof prixNormal != 'undefined'){ outputNormal = prixNormal; } %>
		<input type="number" step="0.1" value="<%= outputNormal %>" min="0" class="form-control inputNormal">
	</div>
	<div class="col-xs-3 prix">
		<% var outputHappyHour = "-"; if(typeof prixHappyHour != 'undefined'){ outputHappyHour = prixHappyHour; } %>
		<input type="number" step="0.1" value="<%= outputHappyHour %>" min="0" class="form-control inputHappyHour">
	</div>
	<div class="col-xs-3 remove">
		<img src="{{ asset("bundles/beertobeercore/images/remove.png") }}" alt="Supprimer">
	</div>
</div>